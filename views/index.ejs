<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <% data.national_parks.forEach(function(p){ %>
    	<p><%= p.name %></p>
    	<p><%= p.state %></p>
    	<p><%= p.cost %></p>
    	<p><img src="<%= p.image_url %>" /></p>
    <% }) %>
  </body>
</html>
